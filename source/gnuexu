# Build the Euphoria Interpreter for Linux - OBSOLETE!
echo compiling all .c files ...
gcc -c -finline-functions -O2  -fomit-frame-pointer -fsigned-char -DELINUX inline.c 
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DELINUX symtab.c  
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DELINUX scanner.c   
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DELINUX parser.c  
gcc -c -O -Os -fno-inline-functions -fomit-frame-pointer -fsigned-char -DELINUX error.c 
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DELINUX alloc.c  
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DELINUX emit.c  
gcc -c -O -Os -fno-inline-functions -fomit-frame-pointer -fsigned-char -DELINUX main.c   
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DELINUX runtime.c 
gcc -c -O -Os -fno-inline-functions -fomit-frame-pointer -fsigned-char -DELINUX rterror.c 
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DELINUX x.c  
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DELINUX w.c  
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DELINUX machine.c 
gcc -c -O2 -fno-inline-functions -fno-defer-pop -fsigned-char -DELINUX callc.c 
echo linking all .o files ...
gcc inline.o symtab.o scanner.o parser.o error.o alloc.o emit.o main.o runtime.o rterror.o x.o w.o machine.o callc.o -lm -ldl -lgpm -oexu
strip exu
echo exu has been created

