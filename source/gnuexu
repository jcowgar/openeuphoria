# Build the Euphoria Interpreter for Linux - OBSOLETE!
echo compiling all .c files ...
gcc -c -finline-functions -O2  -fomit-frame-pointer -fsigned-char -DEUNIX inline.c 
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DEUNIX symtab.c  
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DEUNIX scanner.c   
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DEUNIX parser.c  
gcc -c -O -Os -fno-inline-functions -fomit-frame-pointer -fsigned-char -DEUNIX error.c 
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DEUNIX alloc.c  
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DEUNIX emit.c  
gcc -c -O -Os -fno-inline-functions -fomit-frame-pointer -fsigned-char -DEUNIX main.c   
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DEUNIX runtime.c 
gcc -c -O -Os -fno-inline-functions -fomit-frame-pointer -fsigned-char -DEUNIX rterror.c 
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DEUNIX x.c  
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DEUNIX w.c  
gcc -c -O2 -fno-inline-functions -fomit-frame-pointer -fsigned-char -DEUNIX machine.c 
gcc -c -O2 -fno-inline-functions -fno-defer-pop -fsigned-char -DEUNIX callc.c 
echo linking all .o files ...
gcc inline.o symtab.o scanner.o parser.o error.o alloc.o emit.o main.o runtime.o rterror.o x.o w.o machine.o callc.o -lm -ldl -lgpm -oexu
strip exu
echo exu has been created

