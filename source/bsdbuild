exu ec.ex ec.ex  
#ecu ec.ex
./emake
mv ec ecu
ls -l ecu

echo moving to euphoria/bin
mv exu $EUDIR/bin
mv backendu $EUDIR/bin
mv ecu $EUDIR/bin

rm int.c compress.c backend.c 0ackend.c emake

